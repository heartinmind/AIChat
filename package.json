{
  "name": "beauty-clinic-chatbot",
  "version": "1.0.0",
  "description": "엘리트 뷰티 클리닉 AI 챗봇 - 상용화 버전",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --project tsconfig.prod.json",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "deploy": "npm run build:prod && gcloud run deploy",
    "docker:build": "docker build -t beauty-clinic-chatbot .",
    "docker:run": "docker run -p 8000:8000 beauty-clinic-chatbot",
    "migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio"
  },
  "keywords": [
    "beauty-clinic",
    "chatbot",
    "korean",
    "typescript",
    "fastapi",
    "vertex-ai",
    "google-cloud",
    "production"
  ],
  "author": "Elite Beauty Clinic Development Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@google-cloud/secret-manager": "^5.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@prisma/client": "^5.0.0",
    "prisma": "^5.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "axios": "^1.5.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "joi": "^17.0.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "multer": "^1.4.5",
    "sharp": "^0.32.0",
    "qrcode": "^1.5.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}